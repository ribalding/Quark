<h1>Newest Questions</h1>

<div class="row">
  <div class="col-sm-9">
    <ul>
    {{#each model as |question|}}
      <li><h4>{{question.question_text}}</h4></li>
    {{/each}}
    </ul>
  </div>
  <div class="col-sm-3 newQuestionBox">
    {{new-question saveQuestion="saveQuestion"}}
  </div>
</div>
